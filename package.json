{
  "author": {
    "name": "Michael Akiliev"
  },
  "bugs": {
    "url": "https://github.com/houd1ni/fela-vue/issues"
  },
  "bundleDependencies": [],
  "deprecated": false,
  "description": "Fela mixin for Vue designed for flexibility yet team-oriented.",
  "homepage": "https://houd1ni.github.io/fela-vue/",
  "keywords": [
    "vue", "vuejs", "fela", "css-in-js", "css", "styles",
    "styling", "easy", "fp", "functional", "structure",
    "strongly-typed", "typescript", "d.ts"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/bundle.esm.js",
    "./cjs": "./dist/bundle.js"
  },
  "name": "fela-vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/houd1ni/fela-vue.git"
  },
  "scripts": {
    "lint": "tslint src/*.ts",
    "test": "npm run gentypes && npm run prod:es && ava",
    "test:report": "nyc npm test && nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "test:lazy": "ava",
    "gentypes": "dts-bundle-generator -o dist/bundle.d.ts src/main.ts",
    "dev": "cross-env NODE_ENV=development BUILD=es rollup -c",
    "prod:cjs": "cross-env NODE_ENV=production BUILD=cjs rollup -c",
    "prod:es": "cross-env NODE_ENV=production BUILD=es rollup -c",
    "prod": "npm run gentypes && npm run prod:es && npm run prod:cjs"
  },
  "version": "2.8.0-beta.1",
  "ava": {
    "files": [
      "./test/specs/*.js"
    ],
    "failFast": true,
    "timeout": "2m",
    "nodeArguments": [
      "--experimental-specifier-resolution=node"
    ]
  },
  "dependencies": {
    "fela": "*",
    "fela-dom": "*",
    "fela-plugin-embedded": "*",
    "fela-plugin-extend": "*",
    "fela-plugin-fallback-value": "*",
    "fela-plugin-prefixer": "*",
    "fela-plugin-unit": "*",
    "pepka": "^0.13.0-beta12"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "lafetch": "0.4.5",
    "ava": "^5.1.0",
    "codecov": "^3.8.2",
    "cross-env": "^7.0.3",
    "dts-bundle-generator": "^7.1.0",
    "fela-monolithic": "^12.2.1",
    "nyc": "^15.1.0",
    "rollup": "^3.6.0",
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.1.0",
    "@rollup/plugin-replace": "^5.0.1",
    "rollup-plugin-resolve-aliases": "^0.3.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.2",
    "typescript": "^4.9.3"
  },
  "types": "./dist/bundle.d.ts",
  "sideEffects": false
}
